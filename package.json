{
  "name": "app-electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && electron-builder",
    "build:mac": "vue-tsc && vite build && electron-builder --mac",
    "build:win": "vue-tsc && vite build && electron-builder --win",
    "build:linux": "vue-tsc && vite build && electron-builder --linux",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint . --fix"
  },
  "build": {
    "appId": "com.app-electron",
    "productName": "app-electron",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@logicflow/core": "^2.1.4",
    "element-plus": "^2.4.2",
    "mind-elixir": "^5.3.7",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.22",
    "electron": "^27.1.2",
    "electron-builder": "^24.9.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.0",
    "globals": "^16.5.0",
    "less": "^4.4.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.47.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^3.1.5"
  }
}
